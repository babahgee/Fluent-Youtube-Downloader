{
  "name": "fluent-youtube-downloader",
  "version": "0.2.1",
  "description": "Fluent-Youtube-Downloader",
  "main": "./dist/app.js",
  "author": {
    "name": "Rohan Kanhaisingh",
    "url": "https://rohankanhaisingh.com"
  },
  "scripts": {
    "build-ts": "tsc --build",
    "build": "electron-builder",
    "clean": "tsc --build --clean",
    "webpack": "webpack --config webpack.config.js",
    "watch": "tsc-watch --project public/scripts/tsconfig.json",
    "start": "electron .",
    "auto-build": "tsc --build && electron .",
    "watch-scss": "node ./scss-config.js"
  },
  "build": {
    "appId": "com.rohankanhaisingh.fluent-youtube-downloader",
    "productName": "Fluent Youtube Downloader",
    "directories": {
      "output": "build"
    },
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        }
      ],
      "icon": "./icon.ico"
    }
  },
  "devDependencies": {
    "@types/node": "^14.14.7",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/parser": "^5.33.0",
    "electron": "^27.1.0",
    "electron-builder": "^24.6.4",
    "eslint": "^8.21.0",
    "ts-loader": "^9.5.1",
    "typescript": "^4.5.2",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
    },
    "plugins": [
      "@typescript-eslint"
    ]
  },
  "dependencies": {
    "@babahgee/easings": "^1.0.0",
    "@types/body-parser": "^1.19.5",
    "@types/cors": "^2.8.16",
    "@types/dotenv": "^8.2.0",
    "@types/ejs": "^3.1.5",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.17.10",
    "@types/portscanner": "^2.1.4",
    "@types/uuid": "^9.0.7",
    "body-parser": "^1.20.2",
    "colors": "^1.4.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "ejs": "^3.1.9",
    "electron-is-dev": "^2.0.0",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "glob": "^10.3.10",
    "node-gyp": "^10.0.1",
    "portscanner": "^2.2.0",
    "sass": "^1.69.5",
    "uuid": "^9.0.1",
    "ytdl-core": "^4.11.5"
  }
}
